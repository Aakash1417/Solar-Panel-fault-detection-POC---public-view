{
    "model": {
        "min_input_size": 400,
        "max_input_size": 400,
        "anchors": [
            5, 7, 10, 14, 15, 15, 26, 32, 45, 119, 54, 18, 94, 59, 109, 183,
            200, 21
        ],
        "labels": ["1"],
        "backend": "pretrained_yolo.h5"
    },
    "train": {
        "train_image_folder": "../trainingImages/images/",
        "train_annot_folder": "../trainingImages/annotations/",
        "cache_name": "../cached/yoloFaultDetection.pkl",

        "train_times": 1,

        "batch_size": 2,
        "learning_rate": 1e-4,
        "nb_epochs": 200,
        "warmup_epochs": 15,
        "ignore_thresh": 0.5,
        "gpus": "0,1",

        "grid_scales": [1, 1, 1],
        "obj_scale": 5,
        "noobj_scale": 1,
        "xywh_scale": 1,
        "class_scale": 1,

        "tensorboard_dir": "log_ouputs",
        "saved_weights_name": "../cached/yolov3Weights.h5",
        "debug": true
    },
    "valid": {
        "valid_image_folder": "../Train&Test_S/Test/images/",
        "valid_annot_folder": "../Train&Test_S/Test/anns/",
        "cache_name": "../cached/val_fault_1.pkl",

        "valid_times": 1
    }
}
